int a[]=new int[n];
int count=0;
System.out.println("enter the nuber of test cases");
int t=sc.nextInt();
System.out.println("enter the value for k");
int k=sc.nextInt();
System.out.println("enter the array elements");
for(int i=0;i<n;i++)
{
   a[i]=sc.nextInt();
}
Arrays.sort(a);
count=k-a[0];
System.out.println(count);
}
}
